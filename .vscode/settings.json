{
    "prettier.jsxBracketSameLine": true,
    "prettier.jsxSingleQuote": true,
    "prettier.printWidth": 200,
    "prettier.proseWrap": "always",
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.useTabs": true,
    "files.associations": {
        "*.js": "javascriptreact"
    },
    "js/ts.implicitProjectConfig.checkJs": false,
    "eslint.enable": true,
    "eslint.validate": [
        "javascript"
    ]
}